%include "stud_io.inc"  ;PRINT 'msg' GETCHAR PUTCHAR al FINISH

global _start

section .text

_start: xor     ecx,    ecx
inp:    GETCHAR
        cmp     al,     10
        jz      mod_num
        inc     ecx
        jmp     inp
mod_num:mov     eax,    ecx     ;prepare
        mov     ecx,    esp
        mov     ebx,    10
dv_prt: xor     edx,    edx     ;begining of division
        div     ebx
        add     edx,    48
        push    edx
        cmp     eax,    0
        jz      prnt
        jmp     dv_prt
prnt:   pop     eax             ;print result
        PUTCHAR al
        cmp     ecx,    esp
        jz      fin
        jmp     prnt
fin:    FINISH
